<p id="notice"><%= notice %></p>

<h1>Site Features</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @site_features.each do |site_feature| %>
      <tr>
        <td><%= link_to 'Show', site_feature %></td>
        <td><%= link_to 'Edit', edit_site_feature_path(site_feature) %></td>
        <td><%= link_to 'Destroy', site_feature, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Site Feature', new_site_feature_path %>
